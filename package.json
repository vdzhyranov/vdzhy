{  
  "name": "@vdzhyranov/dzhy",
  "author": "vDzhyranov",
  "version": "0.1.0",
    "publishConfig": {
    "registry": "https://npm.pkg.github.com/@vdzhyranov"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsdx build && yarn build-tailwind",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "storybook": "start-storybook -p 3000",
    "build-storybook": "build-storybook",
    "build-tailwind": "NODE_ENV=production npx tailwindcss -o ./dist/tailwind.css --minify"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/dzhy.esm.js",
  "size-limit": [
    {
      "path": "dist/dzhy.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/dzhy.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@headlessui/react": "1.7.8",
    "@babel/core": "7.20.12",
    "@size-limit/preset-small-lib": "8.1.2",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "6.5.16",
    "@storybook/react": "6.5.16",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "10.4.13",
    "babel-loader": "9.1.2",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "size-limit": "8.1.2",
    "tailwindcss": "3.2.4",
    "tsdx": "0.14.1",
    "tslib": "2.5.0",
    "typescript": "4.9.4"
  }
}
